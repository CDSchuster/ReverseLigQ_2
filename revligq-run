#!/usr/bin/env python3
import argparse
import logging


def main():
    parser = argparse.ArgumentParser(description="A sample script.")
    parser.add_argument('-arg1', type=str, help="Argument 1")
    parser.add_argument('-arg2', type=str, help="Argument 2")
    args = parser.parse_args()

 
if __name__ == "__main__":
    main()